{
  "name": "better-emmet-plugin",
  "version": "1.0.0-beta.3",
  "description": "Emmet abbreviation parser for better-dom",
  "main": "dist/better-emmet-plugin.js",
  "config": {
    "gulp": "--gulpfile node_modules/better-dom-boilerplate/gulpfile.js --cwd ."
  },
  "directories": {
    "test": "test"
  },
  "peerDependencies": {
    "better-dom": "^3.0.0-beta.3"
  },
  "devDependencies": {
    "better-dom": "^3.0.0-beta.3",
    "better-dom-boilerplate": "git://github.com/chemerisuk/better-dom-boilerplate.git"
  },
  "scripts": {
    "test": "gulp test $npm_package_config_gulp",
    "start": "gulp dev $npm_package_config_gulp",
    "version": "gulp dist $npm_package_config_gulp && git add -A dist bower.json",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chemerisuk/better-emmet-plugin.git"
  },
  "keywords": [
    "ecosystem:better-dom",
    "better-emmet-plugin",
    "emmet",
    "html"
  ],
  "author": "Maksim Chemerisuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chemerisuk/better-emmet-plugin/issues"
  },
  "homepage": "https://github.com/chemerisuk/better-emmet-plugin",
  "karmaFiles": [
    "node_modules/better-dom/dist/better-dom.js"
  ]
}
